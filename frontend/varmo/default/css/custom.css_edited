}
.owl-carousel .owl-dots span,
.owl-carousel .owl-dots .owl-dot.active span, 
.owl-carousel .owl-dots.clickable .owl-dot:hover span{
	-webkit-transition: all 350ms ease 0s;
	-moz-transition: all 350ms ease 0s;
